generator client {
  provider     = "prisma-client"
  output       = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String  @id @default(cuid())
  uid       String  @unique
  email     String
  name      String?
  createdAt DateTime @default(now())
}

